	<body class="dark bg-slate-800 text-gray-50">
		<script>
			window.GRAPH_DATA = '{{ graph_data | safe }}';
			window.PAGE_TITLE = '{{ page_title }}';
		</script>
		<div id="app"></div>
	</body>
</html>
