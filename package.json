  },
  "devDependencies": {
    "@types/node": "^22.19.11",
    "babel-preset-moon": "^3.1.4",
    "eslint": "^9.23.0",
    "eslint-config-moon": "^4.0.0",
    "execa": "^6.1.0",
    "jest": "^29.7.0",
    "jest-preset-moon": "^3.1.0",
    "packemon": "^4.1.2",
    "prettier": "^3.8.1",
    "prettier-config-moon": "^1.2.2",
    "tsconfig-moon": "^1.4.2",
    "typescript": "5.9.3"
  }
}
