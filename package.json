{
  "name": "moon",
  "private": true,
  "packageManager": "yarn@4.12.0",
  "scripts": {
    "moon": "target/debug/moon --log trace",
    "type": "target/debug/moon --log trace run :typecheck",
    "clean": "rm -rf packages/*/{cjs,dist,dts,.eslintcache,tsconfig.*.tsbuildinfo}"
